  "scripts": {
    "test": "yarn run test:hardhat && yarn run test:forge",
    "test:hardhat": "hardhat test",
    "test:forge": "eval $(grep '^INFURA_STAGING_KEY' .env) && forge test --fork-url https://polygon-mumbai.infura.io/v3/${INFURA_STAGING_KEY}",
    "test:gas": "REPORT_GAS=true hardhat test --deploy-fixture",
    "test:coverage": "CI=true hardhat coverage --solcoverjs ./.solcover.js",
    "test:unsafe": "OPTIMIZER=false VIA_IR=false TS_NODE_TRANSPILE_ONLY=1 yarn test --no-compile",
    "prepare": "foundryup && yarn build",
    "typechain": "TS_NODE_TRANSPILE_ONLY=1 hardhat typechain",
    "build": "yarn run typechain && hardhat compile",
    "clean": "TS_NODE_TRANSPILE_ONLY=1 hardhat clean",
    "deploy": "TRACE=true LOG_HARDHAT_NETWORK=true hardhat deploy",
    "node:verbose": "TRACE=true LOG_HARDHAT_NETWORK=true hardhat node"
  }
